          <%=chef[i].starter%>
        </h4>
        <h4>
          Main:
          <%=chef[i].main%>
        </h4>
        <h4>
          Dessert:
          <%=chef[i].dessert%>
        </h4>
        <h4>
          Drinks:
          <%=chef[i].drink%>
